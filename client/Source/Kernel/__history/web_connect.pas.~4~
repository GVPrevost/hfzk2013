unit web_connect;

interface

uses mshtml, OleCtrls, SHDocVw,forms;

function Get(url:string):string;

implementation

function Get(url:string):string;
var
  wb: TWebBrowser;
  i: IHTMLElement;
begin
  wb := TWebBrowser.Create(application);
  wb.Navigate(url);
  i:=(wb.Document as IHTMLDocument2).body;
  result:=i.outerText;
  wb.Free;
end;

end.
