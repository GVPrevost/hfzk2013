unit web_connect;

interface

uses mshtml, OleCtrls, SHDocVw,forms;

function Get(url:string):string;

implementation

uses NativeXML;

function Get(url:string):string;
var
  wb: TWebBrowser;
  i: IHTMLElement;
  xml:TNativeXML;
begin
  xml:=TNativeXML.Create(nil);
  xml.LoadFromURL(url);
  result:=xml.WriteToString;
  xml.Free;
end;

end.
